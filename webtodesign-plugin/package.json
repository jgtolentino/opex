{
  "name": "webtodesign-figma-plugin",
  "version": "1.0.0",
  "description": "Convert any website to editable Figma designs",
  "author": "Jake Tolentino",
  "license": "MIT",
  "scripts": {
    "build": "tsc && npm run copy-ui",
    "copy-ui": "cp src/ui.html dist/ui.html",
    "watch": "tsc --watch",
    "dev": "concurrently \"npm run watch\" \"npm run copy-ui\"",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.90.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0"
  },
  "dependencies": {}
}
